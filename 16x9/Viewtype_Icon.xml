    <include name="View_Icon">
        <param name="updownid" default="true" />
        <param name="width" default="headlinew" />
        <param name="iconitemw" default="iconitemw" />
        <param name="icongenreitemw" default="icongenreitemw" />
        <param name="iconpadw" default="iconpadw" />
        <param name="iconpanelpadw" default="iconpanelpadw" />
        <definition>
            <control type="panel" id="$PARAM[id]">
                <top>15</top>
                <left>$PARAM[iconpanelpadw]</left>
                <width>$PARAM[width]</width>
                <height>690</height>
                <onup condition="$PARAM[updownid]">$PARAM[id]</onup>
                <ondown condition="$PARAM[updownid]">$PARAM[id]</ondown>
                <onup condition="!$PARAM[updownid]">$PARAM[onup]</onup>
                <ondown condition="!$PARAM[updownid]">$PARAM[ondown]</ondown>
                <onright condition="!Skin.HasSetting(KioskMode)">SetFocus(9000)</onright>
                <onleft>60</onleft>
                <onback condition="!String.IsEmpty(Window(home).Property(HubReturn)) + String.IsEqual(Container.FolderPath,Window(home).Property(WindowBaseReturn))">AlarmClock(returnhack,ActivateWindow($INFO[Window(home).Property(HubReturn)]),00:00,silent)</onback>
                <pagecontrol>60</pagecontrol>
                <preloaditems>2</preloaditems>
                <orientation>vertical</orientation>
                <viewtype label="$PARAM[name]">icons</viewtype>
                <scrolltime tween="quadratic">400</scrolltime>
                <visible>$PARAM[condition]</visible>
                <itemlayout height="345" width="$PARAM[iconitemw]" condition="![Container.Content(genres) | Container.Content(years) | Container.Content(tags) | Container.Content(studios) | Container.Content(countries) | Container.Content(directors)]">
                    <control type="group">
                        <left>$PARAM[iconpadw]</left>
                        <right>$PARAM[iconpadw]</right>
                        <bottom>5</bottom>
                        <top>5</top>
                        <control type="group">
                            <left>pad</left>
                            <right>pad</right>
                            <top>pad</top>
                            <bottom>100</bottom>
                            <control type="textbox">
                                <label>$INFO[ListItem.Label]</label>
                                <font>Font-OSD</font>
                                <align>center</align>
                                <height>64</height>
                                <bottom>-80</bottom>
                                <visible>String.IsEmpty(ListItem.Art(tvshow.poster)) + String.IsEmpty(ListItem.Art(poster))</visible>
                            </control>
                            <control type="image">
                                <include condition="!Skin.HasSetting(global.hideobjectshadows) + !String.IsEqual(Skin.CurrentColourTheme,Light)">Object_Background_Shadows</include>
                                <aspectratio aligny="center">keep</aspectratio>
                                <texture background="true" fallback="DefaultFile.png">$VAR[Image_Poster]</texture>
                                <visible>String.IsEmpty(ListItem.Art(tvshow.poster)) + String.IsEmpty(ListItem.Art(poster))</visible>
                            </control>
                        </control>
                        <control type="group">
                            <left>pad</left>
                            <right>pad</right>
                            <top>pad</top>
                            <bottom>pad</bottom>
                            <control type="image">

                                <include condition="!Skin.HasSetting(global.hideobjectshadows) + !String.IsEqual(Skin.CurrentColourTheme,Light)">Object_Background_Shadows</include>
                                <aspectratio>scale</aspectratio>
                                <texture background="true">$VAR[Image_Poster]</texture>
                                <visible>![String.IsEmpty(ListItem.Art(tvshow.poster)) + String.IsEmpty(ListItem.Art(poster))]</visible>
                            </control>
                        </control>
                        <control type="image">
                            <right>15</right>
                            <top>15</top>
                            <height>32</height>
                            <width>32</width>
                            <texture colordiffuse="ListLabel">$VAR[Image_Indicator]</texture>
                            <visible>!String.IsEmpty(ListItem.Overlay)</visible>
                        </control>
                    </control>
                </itemlayout>
                <itemlayout height="115" width="$PARAM[icongenreitemw]" condition="[Container.Content(genres) | Container.Content(years) | Container.Content(tags) | Container.Content(studios) | Container.Content(countries) | Container.Content(directors)]">
                    <control type="group">
                        <left>$PARAM[iconpadw]</left>
                        <right>$PARAM[iconpadw]</right>
                        <bottom>5</bottom>
                        <top>5</top>
                        <control type="group">
                            <left>pad</left>
                            <right>pad</right>
                            <top>pad</top>
                            <bottom>pad</bottom>
                            <control type="image">
                                <!-- <include condition="!Skin.HasSetting(global.hideobjectshadows) + !String.IsEqual(Skin.CurrentColourTheme,Light)">Object_Background_Shadows</include> -->
                                <aspectratio aligny="center">scale</aspectratio>
                                <texture background="true" colordiffuse="PosterBorder">common/white.png</texture>
                            </control>
                            <control type="label">
                                <label>$INFO[ListItem.Label]</label>
                                <align>center</align>
                                <left>pad</left>
                                <right>pad</right>
                            </control>
                        </control>
                    </control>
                </itemlayout>
                <focusedlayout height="345" width="$PARAM[iconitemw]" condition="![Container.Content(genres) | Container.Content(years) | Container.Content(tags) | Container.Content(studios) | Container.Content(countries) | Container.Content(directors)]">
                    <control type="group">
                        <left>$PARAM[iconpadw]</left>
                        <right>$PARAM[iconpadw]</right>
                        <bottom>5</bottom>
                        <top>5</top>
                        <control type="group">
                            <left>pad</left>
                            <right>pad</right>
                            <top>pad</top>
                            <bottom>100</bottom>
                            <control type="textbox">
                                <label>$INFO[ListItem.Label]</label>
                                <font>Font-OSD</font>
                                <align>center</align>
                                <height>64</height>
                                <bottom>-80</bottom>
                                <visible>String.IsEmpty(ListItem.Art(tvshow.poster)) + String.IsEmpty(ListItem.Art(poster))</visible>
                            </control>
                            <control type="image">
                                <left>-10</left>
                                <right>-10</right>
                                <top>-10</top>
                                <bottom>-10</bottom>
                                <bordersize>10</bordersize>
                                <bordertexture colordiffuse="$VAR[HighlightColor]" border="10">common/border.png</bordertexture>
                                <aspectratio aligny="center">keep</aspectratio>
                                <texture background="true" fallback="DefaultFile.png">$VAR[Image_Poster]</texture>
                                <visible>String.IsEmpty(ListItem.Art(tvshow.poster)) + String.IsEmpty(ListItem.Art(poster))</visible>
                            </control>

                        </control>
                        <control type="image">
                            <bordersize>10</bordersize>
                            <bordertexture colordiffuse="$VAR[HighlightColor]" border="10">common/border.png</bordertexture>
                            <aspectratio>scale</aspectratio>
                            <texture background="true">$VAR[Image_Poster]</texture>
                            <visible>![String.IsEmpty(ListItem.Art(tvshow.poster)) + String.IsEmpty(ListItem.Art(poster))]</visible>
                        </control>
                        <control type="image">
                            <right>15</right>
                            <top>15</top>
                            <height>32</height>
                            <width>32</width>
                            <texture colordiffuse="ListLabel">$VAR[Image_Indicator]</texture>
                            <visible>!String.IsEmpty(ListItem.Overlay)</visible>
                        </control>
                    </control>
                </focusedlayout>
                <focusedlayout height="115" width="$PARAM[icongenreitemw]" condition="[Container.Content(genres) | Container.Content(years) | Container.Content(tags) | Container.Content(studios) | Container.Content(countries) | Container.Content(directors)]">
                    <control type="group">
                        <left>$PARAM[iconpadw]</left>
                        <right>$PARAM[iconpadw]</right>
                        <bottom>5</bottom>
                        <top>5</top>
                        <control type="group">
                            <left>pad</left>
                            <right>pad</right>
                            <top>pad</top>
                            <bottom>pad</bottom>
                            <control type="image">
                                <texture background="true" colordiffuse="$VAR[HighlightColor]">common/white.png</texture>
                            </control>
                            <control type="label">
                                <label>$INFO[ListItem.Label]</label>
                                <align>center</align>
                                <left>pad</left>
                                <right>pad</right>
                                <textcolor>Selected</textcolor>
                            </control>
                        </control>
                    </control>
                </focusedlayout>
            </control>
        </definition>
    </include>

