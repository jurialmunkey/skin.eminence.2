<?xml version="1.0" encoding="UTF-8"?>
<window>
    <defaultcontrol>150</defaultcontrol>
    <controls>
        <control type="group">
            <top>dialogtop</top>
            <bottom>132</bottom>
            <left>sidepad</left>
            <right>sidepad</right>
            <include>Animation_SlideIn</include>
            <include>Animation_FadeOut</include>
            <animation effect="slide" tween="quadratic" easing="out" time="300" start="0,1920" end="0">WindowOpen</animation>
            <animation effect="slide" tween="quadratic" easing="in" time="300" end="0,1920" start="0">WindowClose</animation>
            <include>Dialog_Background</include>
            <control type="group">
                <top>70</top>
                <left>bigpad</left>
                <right>bigpad</right>
                <control type="group">
                    <width>460</width>
                    <height>648</height>
                    <include>Object_FlatBackground</include>
                    <include content="Object_Line">
                        <param name="width" value="460" />
                        <param name="label" value="$INFO[Control.GetLabel(100)]" />
                    </include>
                    <control type="button" id="160">
                        <align>left</align>
                        <width>460</width>
                        <label>413</label>
                        <ondown>150</ondown>
                        <onright>120</onright>
                        <font>Font-ListInfo-Small-Bold</font>
                        <include>Defs_OptionButton</include>
                        <visible>Control.IsEnabled(160)</visible>
                    </control>
                    <control type="panel" id="150">
                        <onup>160</onup>
                        <right>0</right>
                        <left>0</left>
                        <top>70</top>
                        <bottom>0</bottom>
                        <onright>120</onright>
                        <scrolltime>200</scrolltime>
                        <orientation>vertical</orientation>
                        <itemlayout width="460" height="72">
                            <control type="label">
                                <left>bigpad</left>
                                <right>30</right>
                                <height>72</height>
                                <textcolor>ListLabel</textcolor>
                                <font>Font-ListInfo-Small-Bold</font>
                                <aligny>center</aligny>
                                <label>$INFO[ListItem.Label]</label>
                            </control>
                        </itemlayout>
                        <focusedlayout width="460" height="72">
                            <control type="image">
                                <width>100%</width>
                                <height>72</height>
                                <texture colordiffuse="$VAR[HighlightColor]">common/white.png</texture>
                            </control>
                            <control type="label">
                                <left>bigpad</left>
                                <right>30</right>
                                <height>72</height>
                                <textcolor>Selected</textcolor>
                                <font>Font-ListInfo-Small-Bold</font>
                                <aligny>center</aligny>
                                <label>$INFO[ListItem.Label]</label>
                            </control>
                        </focusedlayout>
                    </control>
                </control>

                <control type="group">
                    <left>480</left>
                    <right>0</right>
                    <height>648</height>
                    <include>Object_FlatBackground</include>
                    <include content="Object_Line">
                        <param name="width" value="listlinew" />
                        <param name="label" value="$INFO[Control.GetLabel(130)]$INFO[Control.GetLabel(140)]$INFO[Player.FileName, - ,]" />
                    </include>
                    <control type="panel" id="120">
                        <right>0</right>
                        <left>0</left>
                        <top>0</top>
                        <bottom>0</bottom>
                        <onleft>150</onleft>
                        <scrolltime>200</scrolltime>
                        <orientation>vertical</orientation>
                        <itemlayout width="listlinew" height="72">
                            <control type="image">
                                <posx>11</posx>
                                <posy>12</posy>
                                <width>44</width>
                                <height>44</height>
                                <texture fallback="DefaultAddonNone.png">subs/$INFO[ListItem.Thumb].png</texture>
                            </control>
                            <control type="image">
                                <left>77</left>
                                <top>22</top>
                                <width>100</width>
                                <height>24</height>
                                <aspectratio align="left">keep</aspectratio>
                                <texture>subs/icon_close_caption.png</texture>
                                <visible>!ListItem.property(hearing_imp)</visible>
                                <colordiffuse>Disabled</colordiffuse>
                                <animation effect="fade" start="100" end="25" condition="true">Conditional</animation>
                            </control>
                            <control type="image">
                                <left>130</left>
                                <top>22</top>
                                <width>100</width>
                                <height>24</height>
                                <aspectratio align="left">keep</aspectratio>
                                <texture>subs/icon_sync.png</texture>
                                <visible>!ListItem.property(sync)</visible>
                                <colordiffuse>Disabled</colordiffuse>
                                <animation effect="fade" start="100" end="25" condition="true">Conditional</animation>
                            </control>
                            <control type="image">
                                <left>77</left>
                                <top>22</top>
                                <width>100</width>
                                <height>24</height>
                                <aspectratio align="left">keep</aspectratio>
                                <colordiffuse>ListLabel</colordiffuse>
                                <texture>subs/icon_close_caption.png</texture>
                                <visible>ListItem.property(hearing_imp)</visible>
                            </control>
                            <control type="image">
                                <left>130</left>
                                <top>22</top>
                                <width>100</width>
                                <height>24</height>
                                <aspectratio align="left">keep</aspectratio>
                                <texture>subs/icon_sync.png</texture>
                                <colordiffuse>ListLabel</colordiffuse>
                                <visible>ListItem.property(sync)</visible>
                            </control>
                            <control type="label">
                                <left>230</left>
                                <right>30</right>
                                <height>72</height>
                                <textcolor>ListLabel</textcolor>
                                <font>Font-ListInfo-Small-Bold</font>
                                <aligny>center</aligny>
                                <label>$INFO[ListItem.Label]$INFO[ListItem.Label2, - ,]</label>
                            </control>
                        </itemlayout>
                        <focusedlayout width="listlinew" height="72">
                            <control type="image">
                                <width>100%</width>
                                <height>72</height>
                                <texture colordiffuse="$VAR[HighlightColor]">common/white.png</texture>
                            </control>
                            <control type="image">
                                <posx>11</posx>
                                <posy>12</posy>
                                <width>44</width>
                                <height>44</height>
                                <texture fallback="DefaultAddonNone.png">subs/$INFO[ListItem.Thumb].png</texture>
                            </control>
                            <control type="image">
                                <left>77</left>
                                <top>22</top>
                                <width>100</width>
                                <height>24</height>
                                <aspectratio align="left">keep</aspectratio>
                                <texture>subs/icon_close_caption.png</texture>
                                <visible>!ListItem.property(hearing_imp)</visible>
                                <colordiffuse>LineLabel</colordiffuse>
                                <animation effect="fade" start="100" end="25" condition="true">Conditional</animation>
                            </control>
                            <control type="image">
                                <left>130</left>
                                <top>22</top>
                                <width>100</width>
                                <height>24</height>
                                <aspectratio align="left">keep</aspectratio>
                                <texture>subs/icon_sync.png</texture>
                                <visible>!ListItem.property(sync)</visible>
                                <colordiffuse>LineLabel</colordiffuse>
                                <animation effect="fade" start="100" end="25" condition="true">Conditional</animation>
                            </control>
                            <control type="image">
                                <left>77</left>
                                <top>22</top>
                                <width>100</width>
                                <height>24</height>
                                <aspectratio align="left">keep</aspectratio>
                                <colordiffuse>Selected</colordiffuse>
                                <texture>subs/icon_close_caption.png</texture>
                                <visible>ListItem.property(hearing_imp)</visible>
                            </control>
                            <control type="image">
                                <left>130</left>
                                <top>22</top>
                                <width>100</width>
                                <height>24</height>
                                <aspectratio align="left">keep</aspectratio>
                                <texture>subs/icon_sync.png</texture>
                                <colordiffuse>Selected</colordiffuse>
                                <visible>ListItem.property(sync)</visible>
                            </control>
                            <control type="label">
                                <left>230</left>
                                <right>30</right>
                                <height>72</height>
                                <textcolor>Selected</textcolor>
                                <font>Font-ListInfo-Small-Bold</font>
                                <aligny>center</aligny>
                                <label>$INFO[ListItem.Label]$INFO[ListItem.Label2, - ,]</label>
                            </control>
                        </focusedlayout>
                    </control>
                </control>

            </control>
        </control>
        <control type="label" id="100"><visible>false</visible></control>
        <control type="label" id="130"><visible>false</visible></control>
        <control type="label" id="140"><visible>false</visible></control>

    </controls>

</window>
